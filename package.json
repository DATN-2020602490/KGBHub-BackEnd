{
  "name": "kgb-hub",
  "version": "1.0.0",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "check-types": "tsc --noEmit --pretty",
    "dbdown": "docker compose -f database/docker-compose.yml down",
    "dbup": "docker compose -f database/docker-compose.yml up -d",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "migrate": "npx prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "rsdb": "yarn dbdown && rimraf database/postgres && rimraf database/redis && yarn dbup",
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": "prettier --write",
    "package.json": "npx -y sort-package-json"
  },
  "dependencies": {
    "@google-analytics/data": "^4.8.0",
    "@prisma/client": "^5.16.1",
    "@react-email/components": "^0.0.7",
    "@react-email/render": "^0.0.7",
    "@supabase/supabase-js": "^2.26.0",
    "@swc/core": "^1.5.27",
    "axios": "^1.5.0",
    "bignumber.js": "^9.1.2",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "get-video-duration": "^4.1.0",
    "google-auth-library": "^9.0.0",
    "google-spreadsheet": "^4.1.2",
    "googleapis": "^124.0.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "open-graph-scraper": "^6.8.1",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.16.1",
    "react": "^18.2.0",
    "remove-accents": "^0.5.0",
    "rimraf": "^5.0.1",
    "sharp": "0.32.0",
    "slug": "^9.1.0",
    "socket.io": "^4.7.5",
    "stripe": "^14.24.0"
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.17.9",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.1",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/react": "^18.2.20",
    "@types/slug": "^5.0.8",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
